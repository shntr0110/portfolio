---
interface Props {
  title: string;
  titleEn?: string;
}

const { title, titleEn } = Astro.props;
---

<h1 class="flex w-fit flex-col">
  <span
    class="font-poppins text-60 font-semibold capitalize leading-100 tracking-02 text-white lg:text-[96px]"
    >{titleEn}</span
  >
  <span class="text-20 font-bold leading-160 tracking-02 text-white">{title}</span>
</h1>
